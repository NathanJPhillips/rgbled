<Application x:Class="Circuitboard7.RgbLed.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="using:Varibale_RGB_LED"
			 RequestedTheme="Dark">
	<Application.Resources>
		<ResourceDictionary>
			<x:String x:Key="AppName">Variable RGB LED</x:String>

			<Style x:Key="TitleTextBlockStyle"
				   TargetType="TextBlock"
				   BasedOn="{StaticResource BaseTextBlockStyle}">
				<Setter Property="FontSize"
						Value="23" />
				<Setter Property="FontWeight"
						Value="ExtraLight" />
				<Setter Property="FontStretch"
						Value="ExtraCondensed" />
			</Style>
		</ResourceDictionary>
	</Application.Resources>
</Application>