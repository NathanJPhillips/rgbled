			 xmlns:local="using:Porrey.RgbLed"
			 xmlns:common="using:Porrey.RgbLed.Converters"
			 RequestedTheme="Dark">
	<Application.Resources>
		<ResourceDictionary>
			<x:String x:Key="AppName">Variable RGB LED</x:String>

			<!-- ***            -->
			<!-- *** Converters -->
			<!-- ***            -->
